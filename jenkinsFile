pipeline {

    agent any
    
   tools{
       maven "maven"
   }
   stages {

       stage ('Compile')
       steps{
       
          sh "mvn clean compile"
       }
       stage("Test"){
          
        steps{
          sh "mvn clean test"
                }
           }
        stage("Deploy"){
          
          sh "maven clean install"
        }
   }
}
 
 